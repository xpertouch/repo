<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#000000</item>
        <item name="colorPrimaryDark">#e20909</item>
        <item name="colorAccent">#000000</item>
        <!--<item name="android:textColorPrimaryInverse">@color/vectorColor-->
        <!--</item>-->
        <item name="android:textColorPrimary">#000000</item>
        <item name="android:colorBackground">#ffffff</item>
        <item name="android:windowTranslucentStatus" tools:targetApi="kitkat">true</item>
        <item name="android:windowBackground">@color/cardview_light_background</item>
        <item name="android:textColorSecondaryInverse">#000000</item>
        <item name="android:textColorSecondary">#000000</item>
        <item name="android:textColorPrimaryInverse">#000000</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="ClickAwayTheme" parent="Theme.AppCompat.Light.NoActionBar">


        <item name="colorPrimary">#000000</item>
        <item name="colorPrimaryDark">#021f29</item>




    </style>

</resources>
